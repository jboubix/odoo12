<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data>
        <record id="dnk_eradicate_duplicate_out_invoice_form_view" model="ir.ui.view">
            <field name="name">dnk.eradicate.duplicate.out.invoice.form.view</field>
            <field name="model">account.invoice</field>
            <field name="inherit_id" ref="account.invoice_form"/>
            <field name="arch" type="xml">
							  <xpath expr="//form" position="attributes">
							      <attribute name="duplicate">0</attribute>
							  </xpath>
            </field>
        </record>

				<record id="dnk_remain_duplicate_out_invoice_form_view" model="ir.ui.view">
						<field name="name">dnk.remain.duplicate.out.invoice.form.view</field>
						<field name="model">account.invoice</field>
						<field name="inherit_id" ref="account.invoice_form"/>
						<field name="groups_id" eval="[(4, ref('dnk_group_account_invoice_duplicate'))]"/>
						<field name="arch" type="xml">
								<xpath expr="//form" position="attributes">
										<attribute name="duplicate">1</attribute>
								</xpath>
						</field>
				</record>
	</data>
</odoo>
