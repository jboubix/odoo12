<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <data>
      <record id="dnk_sale_product_costs_form" model="ir.ui.view">
          <field name="name">dnk_sale_product_costs_form</field>
          <field name="model">res.company</field>
          <field name="inherit_id" ref="base.view_company_form"/>
          <field name="arch" type="xml">
              <xpath expr="//field[@name='currency_id']" position="after">
                  <field class="oe_edit_only" name="dnk_usd_cost_fixed_rate"/>
              </xpath>
          </field>
      </record>
  </data>
</odoo>
