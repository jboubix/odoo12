<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <data>
    <record id="dnk_stock_move_tree_view" model="ir.ui.view">
      <field name="name">dnk.stock.move.tree.view</field>
      <field name="model">stock.move</field>
      <field name="inherit_id" ref="stock.view_move_tree"/>
      <field name="arch" type="xml">
          <field name="date" position="after">
              <field name="dnk_mo_date_planned_start"/>
          </field>
      </field>
    </record>

    <record id="dnk_stock_move_form_view" model="ir.ui.view">
      <field name="name">dnk.stock.move.form.view</field>
      <field name="model">stock.move</field>
      <field name="inherit_id" ref="stock.view_move_form"/>
      <field name="arch" type="xml">
          <field name="date" position="after">
              <field name="dnk_mo_date_planned_start"/>
          </field>
      </field>
    </record>
  </data>
</odoo>
